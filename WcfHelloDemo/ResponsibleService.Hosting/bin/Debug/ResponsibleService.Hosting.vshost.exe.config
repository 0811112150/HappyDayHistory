<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.serviceModel>
		<bindings>
			<netMsmqBinding>
				<binding name="MsmqBinding" exactlyOnce="false" useActiveDirectory="false">
					<security>
						<transport msmqAuthenticationMode="None" msmqProtectionLevel="None">
						</transport>
					</security>
				</binding>
			</netMsmqBinding>
		</bindings>
		<services>
			<service name="ResponsibleService.Service.OrderProcessorService">
				<endpoint bindingConfiguration="MsmqBinding" address="net.msmq://localhost/private/orderprocessor" contract="ResponsibleService.Contract.IOrderProcesser" binding="netMsmqBinding">
				</endpoint>
			</service>
		</services>
	</system.serviceModel>
	<appSettings>
		<add key="msmqPath" value=".\private$\orderprocessor"/>
	</appSettings>
</configuration>